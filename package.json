{
  "name": "cloud-doc",
  "private": true,
  "version": "0.0.0",
  "main": "main.cjs",
  "description": "Online Markdown Editor using Qiniu cloud service",
  "repository": "",
  "type": "module",
  "author": {
    "name": "Hu",
    "email": "840326521@qq.com"
  },
  "scripts": {
    "dev": "vite && onlyEle",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron-dev": "",
    "onlyEle": "nodemon --watch main.cjs --watch electronConfig/* --exec \"electron .\"",
    "start": "concurrently \"npm run dev\" \"npm run onlyEle\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "cloudDoc",
    "productName": "七牛云文档",
    "copyright": "Copyright © 2023 ${author}",
    "files": [
      "./index.html",
      "./main.cjs",
      "node_modules/**/*"
    ],
    "extends": null
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "antd": "^5.16.1",
    "axios": "^1.2.2",
    "bootstrap": "^5.2.3",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "easymde": "^2.18.0",
    "electron": "^30.0.3",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "eslint": "^8.0.1",
    "global": "^4.4.0",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "nodemon": "^3.0.2",
    "qiniu": "^7.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.3.2",
    "react-simplemde-editor": "^5.2.0",
    "uuid": "^9.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-store": "^3.2.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "less": "^4.1.3",
    "typescript": "*",
    "vite": "^4.0.0",
    "vite-plugin-electron": "^0.11.1"
  }
}
